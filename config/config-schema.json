{
  "type": "object",
  "required": true,
  "description": "create a local-development.json file to fill in development-specific vars",
  "properties": {
    "port": {
      "type": "string",
      "description": "Which port to serve up the application on",
      "required": true
    },
    "ghClientId": {
      "type": "string",
      "description": "Github client_id for OAUTH",
      "required": true
    },
    "ghClientSecret": {
      "type": "string",
      "description": "Github client_secret for OAUTH",
      "required": true
    },
    "secret": {
      "type": "string",
      "description": "For signing tokens",
      "required": true
    },
    "awsAccessKey": {
      "type": "string",
      "description": "For connecting to s3",
      "required": true
    },
    "awsSecretAccessKey": {
      "type": "string",
      "description": "For connecting to s3",
      "required": true
    }
  }
}
